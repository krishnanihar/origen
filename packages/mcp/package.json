{
  "name": "@origen/mcp",
  "version": "0.1.0",
  "description": "MCP server for Origen design system",
  "type": "module",
  "main": "./dist/server.js",
  "types": "./dist/server.d.ts",
  "exports": {
    ".": {
      "types": "./dist/server.d.ts",
      "import": "./dist/server.js"
    }
  },
  "files": [
    "dist",
    "src/data"
  ],
  "scripts": {
    "build": "next build",
    "build:lib": "tsup",
    "dev": "next dev",
    "clean": "rm -rf dist .next",
    "lint": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "fetch-to-node": "^2.0.0",
    "next": "^15.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@types/node": "^22.19.3",
    "@types/react": "^19.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.4.0",
    "vitest": "^2.0.0"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "design-system",
    "ai"
  ],
  "license": "MIT"
}
